{
  "name": "mono-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "dev:app": "pnpm --filter @mono-react/app dev",
    "dev:suspense": "pnpm --filter @mono-react/suspense-loading dev",
    "ssr:build": "pnpm --filter @mono-react/ssr-minimum-react16 ssr:build",
    "dev:flash": "pnpm --filter @mono-react/flashlight-effect dev",
    "rest-axios-recoil": "pnpm --filter @mono-react/rest-axios-recoil",
    "react-router-tutorial-v6": "pnpm --filter @mono-react/react-router-tutorial-v6",
    "mobile-web": "pnpm --filter @mono-react/mobile-web"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16.16.0",
    "pnpm": ">=7.16.0"
  },
  "packageManager": "pnpm@7.16.0",
  "devDependencies": {
    "pnpm": "^7.9.0"
  },

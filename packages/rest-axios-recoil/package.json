{
  "name": "@mono-react/rest-axios-recoil",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "jsontiny": "node ./server/apidocs/jsontiny.cjs",
